<p id="notice"><%= notice %></p>

<h1>Line Items</h1>

<table>
  <thead>
    <tr>
      <th>Product Id&nbsp&nbsp</th>
      <th>User Id&nbsp&nbsp</th>
      <th>Qty</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @line_items.each do |line_item| %>
      <tr>
        <td><%= line_item.store_id %></td>
        <td><%= line_item.user_id %></td>
        <td><%= line_item.qty %></td>
        <td><%= link_to 'Show', line_item %></td>
      </tr>
    <% end %>
  </tbody>
</table>
